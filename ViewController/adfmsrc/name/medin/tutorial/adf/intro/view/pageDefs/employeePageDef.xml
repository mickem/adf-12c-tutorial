<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.2.66.68" id="employeePageDef"
                Package="name.medin.tutorial.adf.intro.view.pageDefs">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="DepartmentsView" RangeSize="25" DataControl="AppModuleDataControl" id="DepartmentsViewIterator"/>
    <iterator Binds="AllJobsView" RangeSize="25" DataControl="AppModuleDataControl" id="AllJobsViewIterator"/>
    <iterator Binds="EmployeesView" RangeSize="25" DataControl="AppModuleDataControl" id="EmployeesViewIterator"/>
    <searchRegion Binds="EmployeesViewIterator" Criteria=""
                  Customizer="oracle.jbo.uicli.binding.JUSearchBindingCustomizer" id="EmployeesViewQuery"/>
  </executables>
  <bindings>
    <tree IterBinding="DepartmentsViewIterator" id="DepartmentsView">
      <nodeDefinition DefName="name.medin.tutorial.adf.intro.model.DepartmentsView" Name="DepartmentsView0">
        <AttrNames>
          <Item Value="DepartmentName"/>
        </AttrNames>
        <Accessors>
          <Item Value="EmployeesView"/>
        </Accessors>
      </nodeDefinition>
      <nodeDefinition DefName="name.medin.tutorial.adf.intro.model.EmployeesView" Name="DepartmentsView1"
                      TargetIterator="${bindings.EmployeesViewIterator}">
        <AttrNames>
          <Item Value="FirstName"/>
          <Item Value="LastName"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <tree IterBinding="AllJobsViewIterator" id="AllJobsView">
      <nodeDefinition DefName="name.medin.tutorial.adf.intro.model.AllJobsView" Name="AllJobsView0">
        <AttrNames>
          <Item Value="JobTitle"/>
        </AttrNames>
        <Accessors>
          <Item Value="EmployeesView"/>
        </Accessors>
      </nodeDefinition>
      <nodeDefinition DefName="name.medin.tutorial.adf.intro.model.EmployeesView" Name="AllJobsView1"
                      TargetIterator="${bindings.EmployeesViewIterator}">
        <AttrNames>
          <Item Value="FirstName"/>
          <Item Value="LastName"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <tree IterBinding="EmployeesViewIterator" id="EmployeesView">
      <nodeDefinition DefName="name.medin.tutorial.adf.intro.model.EmployeesView" Name="EmployeesView0">
        <AttrNames>
          <Item Value="FirstName"/>
          <Item Value="LastName"/>
          <Item Value="Email"/>
          <Item Value="Gravatar"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <attributeValues IterBinding="EmployeesViewIterator" id="Gravatar">
      <AttrNames>
        <Item Value="Gravatar"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="EmployeesViewIterator" id="FirstName">
      <AttrNames>
        <Item Value="FirstName"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="EmployeesViewIterator" id="LastName">
      <AttrNames>
        <Item Value="LastName"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="EmployeesViewIterator" id="Email">
      <AttrNames>
        <Item Value="Email"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="EmployeesViewIterator" id="PhoneNumber">
      <AttrNames>
        <Item Value="PhoneNumber"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="EmployeesViewIterator" id="ManagerId">
      <AttrNames>
        <Item Value="ManagerId"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="EmployeesViewIterator" id="DepartmentId">
      <AttrNames>
        <Item Value="DepartmentId"/>
      </AttrNames>
    </attributeValues>
  </bindings>
</pageDefinition>
