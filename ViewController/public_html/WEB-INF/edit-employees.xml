<?xml version="1.0" encoding="ISO-8859-1" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="edit-employees">
    <default-activity>createOrEdit</default-activity>
    <transaction>
      <new-transaction/>
    </transaction>
    <input-parameter-definition id="__2">
      <name>action</name>
      <value>#{pageFlowScope.action}</value>
      <class>java.lang.String</class>
      <required/>
    </input-parameter-definition>
    <view id="edit.jsf">
      <page>/edit.jsf</page>
    </view>
    <method-call id="Commit">
      <method>#{bindings.Commit.execute}</method>
      <outcome>
        <fixed-outcome>Commit</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="Rollback">
      <method>#{bindings.Rollback.execute}</method>
      <outcome>
        <fixed-outcome>Rollback</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-return id="return">
      <outcome>
        <name>return</name>
        <rollback/>
      </outcome>
    </task-flow-return>
    <router id="createOrEdit">
      <case id="__13">
        <expression>#{pageFlowScope.action == 'edit'}</expression>
        <outcome>edit</outcome>
      </case>
      <case id="__15">
        <expression>#{pageFlowScope.action == 'create'}</expression>
        <outcome>create</outcome>
      </case>
      <default-outcome>create</default-outcome>
    </router>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>edit.jsf</from-activity-id>
      <control-flow-case id="save">
        <from-outcome>save</from-outcome>
        <to-activity-id>Commit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="cancel">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>Rollback</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>Commit</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>Commit</from-outcome>
        <to-activity-id>return</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>Rollback</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>Rollback</from-outcome>
        <to-activity-id>return</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>createOrEdit</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>create</from-outcome>
        <to-activity-id>CreateInsert</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__11">
        <from-outcome>edit</from-outcome>
        <to-activity-id>edit.jsf</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>edit.jsf</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
