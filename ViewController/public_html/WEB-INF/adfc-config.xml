<?xml version="1.0" encoding="ISO-8859-1" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="employee">
    <page>/employee.jsf</page>
  </view>
  <task-flow-call id="employee-definition">
    <task-flow-reference>
      <document>/WEB-INF/employee-definition.xml</document>
      <id>employee-definition</id>
    </task-flow-reference>
    <input-parameter id="__6">
      <name>action</name>
      <value>#{EmployeeHelper.action}</value>
    </input-parameter>
  </task-flow-call>
  <control-flow-rule id="__1">
    <from-activity-id>employee</from-activity-id>
    <control-flow-case id="__2">
      <from-outcome>edit</from-outcome>
      <to-activity-id>employee-definition</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__3">
    <from-activity-id>employee-definition</from-activity-id>
    <control-flow-case id="__4">
      <from-outcome>return</from-outcome>
      <to-activity-id>employee</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__5">
    <managed-bean-name>EmployeeHelper</managed-bean-name>
    <managed-bean-class>name.medin.tutorial.adf.intro.view.EmployeeHelper</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</adfc-config>
