<?xml version="1.0" encoding="ISO-8859-1" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="employees">
    <page>/employees.jsf</page>
  </view>
  <task-flow-call id="edit-employees">
    <task-flow-reference>
      <document>/WEB-INF/edit-employees.xml</document>
      <id>edit-employees</id>
    </task-flow-reference>
  </task-flow-call>
  <control-flow-rule id="__1">
    <from-activity-id>employees</from-activity-id>
    <control-flow-case id="__2">
      <from-action>editEmployee</from-action>
      <to-activity-id>edit-employees</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__3">
    <from-activity-id>edit-employees</from-activity-id>
    <control-flow-case id="__4">
      <from-outcome>return</from-outcome>
      <to-activity-id>employees</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
</adfc-config>
