<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE Entity SYSTEM "jbo_03_01.dtd">
<!---->
<Entity
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Employees"
  Version="12.1.2.66.68"
  InheritPersonalization="merge"
  DBObjectType="table"
  DBObjectName="EMPLOYEES"
  AliasName="Employees"
  BindingStyle="OracleName"
  xmlns:validation="http://xmlns.oracle.com/adfm/validation"
  RowClass="name.medin.tutorial.adf.intro.model.EmployeesImpl">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Init|Access"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Attribute
    Name="EmployeeId"
    IsNotNull="true"
    Precision="6"
    Scale="0"
    ColumnName="EMPLOYEE_ID"
    SQLType="NUMERIC"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    TableName="EMPLOYEES"
    PrimaryKey="true"/>
  <Attribute
    Name="FirstName"
    Precision="20"
    ColumnName="FIRST_NAME"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="EMPLOYEES">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="LastName"
    IsNotNull="true"
    Precision="25"
    ColumnName="LAST_NAME"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="EMPLOYEES">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="25"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="Email"
    IsUnique="true"
    IsNotNull="true"
    Precision="25"
    ColumnName="EMAIL"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="EMPLOYEES">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="25"/>
    </DesignTime>
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[(FirstName==null?"firstname":FirstName) + "." + (LastName==null?"lastname":LastName) + "@conencta.se"]]></TransientExpression>
  </Attribute>
  <Attribute
    Name="PhoneNumber"
    Precision="20"
    ColumnName="PHONE_NUMBER"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="EMPLOYEES">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="HireDate"
    IsNotNull="true"
    ColumnName="HIRE_DATE"
    SQLType="DATE"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    TableName="EMPLOYEES">
    <TransientExpression><![CDATA[adf.currentDate]]></TransientExpression>
    <Properties>
      <SchemaBasedProperties>
        <FMT_FORMAT
          ResId="name.medin.tutorial.adf.intro.model.Employees.HireDate_FMT_FORMAT"/>
        <FMT_FORMATTER
          ResId="name.medin.tutorial.adf.intro.model.Employees.HireDate_FMT_FORMATTER"/>
      </SchemaBasedProperties>
    </Properties>
  </Attribute>
  <Attribute
    Name="JobId"
    IsNotNull="true"
    Precision="10"
    ColumnName="JOB_ID"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="EMPLOYEES">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="Salary"
    Precision="8"
    Scale="2"
    ColumnName="SALARY"
    SQLType="NUMERIC"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    TableName="EMPLOYEES">
    <validation:PreDefinedValidationBean
      Name="SalaryPrecisionScale0"
      ResId="name.medin.tutorial.adf.intro.model.Employees.Salary_Rule_0"
      Description="Precision : (8,2)"
      ValName="Precision : (8,2)"
      Subtype="PRECISION_SCALE"/>
    <validation:ExpressionValidationBean
      Name="MaximumSalaryRule"
      OperandType="EXPR"
      Inverse="false">
      <validation:MsgIds>
        <validation:Item
          Value="error.maximu.salary"/>
      </validation:MsgIds>
      <validation:ResExpressions>
        <validation:Expression
          Name="1"><![CDATA[source.hints.Salary.label]]></validation:Expression>
        <validation:Expression
          Name="2"><![CDATA[newValue]]></validation:Expression>
        <validation:Expression
          Name="3"><![CDATA[JobId == "IT_PROG" ? 50000 : 50]]></validation:Expression>
      </validation:ResExpressions>
      <validation:TransientExpression><![CDATA[JobId == "IT_PROG" ? newValue >= 50000 : newValue >= 50]]></validation:TransientExpression>
    </validation:ExpressionValidationBean>
  </Attribute>
  <Attribute
    Name="CommissionPct"
    Precision="2"
    Scale="2"
    ColumnName="COMMISSION_PCT"
    SQLType="NUMERIC"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    TableName="EMPLOYEES"/>
  <Attribute
    Name="ManagerId"
    Precision="6"
    Scale="0"
    ColumnName="MANAGER_ID"
    SQLType="NUMERIC"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    TableName="EMPLOYEES"/>
  <Attribute
    Name="DepartmentId"
    Precision="4"
    Scale="0"
    ColumnName="DEPARTMENT_ID"
    SQLType="NUMERIC"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    TableName="EMPLOYEES"/>
  <AccessorAttribute
    Name="Departments"
    Association="name.medin.tutorial.adf.intro.model.DeptMgrFkAssoc"
    AssociationEnd="name.medin.tutorial.adf.intro.model.DeptMgrFkAssoc.Departments"
    AssociationOtherEnd="name.medin.tutorial.adf.intro.model.DeptMgrFkAssoc.Employees"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <AccessorAttribute
    Name="Employees"
    Association="name.medin.tutorial.adf.intro.model.EmpManagerFkAssoc"
    AssociationEnd="name.medin.tutorial.adf.intro.model.EmpManagerFkAssoc.Employees"
    AssociationOtherEnd="name.medin.tutorial.adf.intro.model.EmpManagerFkAssoc.ManagerIdEmployees"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <AccessorAttribute
    Name="ManagerIdEmployees"
    Association="name.medin.tutorial.adf.intro.model.EmpManagerFkAssoc"
    AssociationEnd="name.medin.tutorial.adf.intro.model.EmpManagerFkAssoc.ManagerIdEmployees"
    AssociationOtherEnd="name.medin.tutorial.adf.intro.model.EmpManagerFkAssoc.Employees"
    Type="oracle.jbo.server.EntityImpl"
    IsUpdateable="true"/>
  <AccessorAttribute
    Name="Departments1"
    Association="name.medin.tutorial.adf.intro.model.EmpDeptFkAssoc"
    AssociationEnd="name.medin.tutorial.adf.intro.model.EmpDeptFkAssoc.Departments1"
    AssociationOtherEnd="name.medin.tutorial.adf.intro.model.EmpDeptFkAssoc.Employees1"
    Type="oracle.jbo.server.EntityImpl"
    IsUpdateable="true"/>
  <Key
    Name="EmpEmailNn">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="EMP_EMAIL_NN"/>
      <Attr Name="_checkCondition" Value='"EMAIL" IS NOT NULL'/>
      <Attr Name="_isCheck" Value="true"/>
    </DesignTime>
    <AttrArray Name="Attributes"/>
  </Key>
  <Key
    Name="EmpEmailUk">
    <DesignTime>
      <Attr Name="_isUnique" Value="true"/>
      <Attr Name="_DBObjectName" Value="EMP_EMAIL_UK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="name.medin.tutorial.adf.intro.model.Employees.Email"/>
    </AttrArray>
  </Key>
  <Key
    Name="EmpEmpIdPk"
    PrimaryKey="true">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="EMP_EMP_ID_PK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="name.medin.tutorial.adf.intro.model.Employees.EmployeeId"/>
    </AttrArray>
  </Key>
  <Key
    Name="EmpHireDateNn">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="EMP_HIRE_DATE_NN"/>
      <Attr Name="_checkCondition" Value='"HIRE_DATE" IS NOT NULL'/>
      <Attr Name="_isCheck" Value="true"/>
    </DesignTime>
    <AttrArray Name="Attributes"/>
  </Key>
  <Key
    Name="EmpJobNn">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="EMP_JOB_NN"/>
      <Attr Name="_checkCondition" Value='"JOB_ID" IS NOT NULL'/>
      <Attr Name="_isCheck" Value="true"/>
    </DesignTime>
    <AttrArray Name="Attributes"/>
  </Key>
  <Key
    Name="EmpLastNameNn">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="EMP_LAST_NAME_NN"/>
      <Attr Name="_checkCondition" Value='"LAST_NAME" IS NOT NULL'/>
      <Attr Name="_isCheck" Value="true"/>
    </DesignTime>
    <AttrArray Name="Attributes"/>
  </Key>
  <Key
    Name="EmpSalaryMin">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="EMP_SALARY_MIN"/>
      <Attr Name="_checkCondition" Value="salary > 0"/>
      <Attr Name="_isCheck" Value="true"/>
    </DesignTime>
    <AttrArray Name="Attributes"/>
  </Key>
  <Key
    Name="EmpDeptFk">
    <DesignTime>
      <Attr Name="_referencedKey" Value="DEPT_ID_PK"/>
      <Attr Name="_isForeign" Value="true"/>
      <Attr Name="_DBObjectName" Value="EMP_DEPT_FK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="name.medin.tutorial.adf.intro.model.Employees.DepartmentId"/>
    </AttrArray>
  </Key>
  <Key
    Name="EmpJobFk">
    <DesignTime>
      <Attr Name="_referencedKey" Value="JOB_ID_PK"/>
      <Attr Name="_isForeign" Value="true"/>
      <Attr Name="_DBObjectName" Value="EMP_JOB_FK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="name.medin.tutorial.adf.intro.model.Employees.JobId"/>
    </AttrArray>
  </Key>
  <Key
    Name="EmpManagerFk">
    <DesignTime>
      <Attr Name="_referencedKey" Value="EMP_EMP_ID_PK"/>
      <Attr Name="_isForeign" Value="true"/>
      <Attr Name="_DBObjectName" Value="EMP_MANAGER_FK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="name.medin.tutorial.adf.intro.model.Employees.ManagerId"/>
    </AttrArray>
  </Key>
  <validation:ExpressionValidationBean
    Name="ValidateEmail"
    Description="Validate the email address"
    OperandType="EXPR"
    Inverse="false">
    <validation:OnAttributes>
      <validation:Item
        Value="FirstName"/>
      <validation:Item
        Value="Email"/>
      <validation:Item
        Value="LastName"/>
    </validation:OnAttributes>
    <validation:OnCondition><![CDATA[FirstName != null && LastName != null]]></validation:OnCondition>
    <validation:MsgIds>
      <validation:Item
        Value="error.email"/>
    </validation:MsgIds>
    <validation:ResExpressions>
      <validation:Expression
        Name="1"><![CDATA[source.hints.Email.label]]></validation:Expression>
      <validation:Expression
        Name="2"><![CDATA[sourceRow.FirstName]]></validation:Expression>
      <validation:Expression
        Name="3"><![CDATA[sourceRow.LastName]]></validation:Expression>
    </validation:ResExpressions>
    <validation:TransientExpression><![CDATA[Email == (FirstName + "." + LastName + "@connecta.se")]]></validation:TransientExpression>
  </validation:ExpressionValidationBean>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="name.medin.tutorial.adf.intro.model.ModelBundle"/>
  </ResourceBundle>
</Entity>
